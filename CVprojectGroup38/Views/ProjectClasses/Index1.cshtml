@model IEnumerable<CVprojectGroup38.Models.ProjectClass>

@using CVprojectGroup38.Models;

@{
    ViewBag.Title = "Projects List";
}

<div style="font-family:Arial">
    <h2>Projects list</h2>
        <ul>
            @foreach (var project in @Model)
            {
                <li>
                    @Html.ActionLink(project.Projectname, "Index1", "UserClass", new { projectid = project.Id }, null);
                </li>
            }
        </ul>
</div>

